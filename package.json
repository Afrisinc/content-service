{
  "name": "backend-template-nodejs",
  "version": "1.0.0",
  "description": "Node.js backend template with Fastify and Prisma",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "tsx src/server.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "format": "prettier --write src/**/*.{js,ts,json}",
    "format:check": "prettier --check src/**/*.{js,ts,json}",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "fix-all": "npm run type-check && npm run lint:fix && npm run format",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:init": "prisma migrate dev --name init",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx src/database/seed.ts",
    "db:setup": "npm run db:migrate:init && npm run db:generate && npm run db:seed",
    "db:reset": "npm run db:migrate:reset && npm run db:seed",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "nodejs",
    "fastify",
    "prisma",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.2.0",
    "@prisma/client": "^5.6.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2",
    "openai": "^6.15.0",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@1.22.22"
}
